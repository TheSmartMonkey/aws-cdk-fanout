{
  "name": "aws-cdk-fanout",
  "description": "CDK contruct for a Fanout pattern with Api Gateway > SNS > SQS > lambda",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "npm run build && npm run deploy",
    "deploy": "cd debug && npm run deploy",
    "undeploy": "cd debug && npm run undeploy",
    "build": "tsc && tsc-alias",
    "fix": "npm run lint && npm run format && npm run typecheck",
    "lint": "eslint src --ext .ts --fix",
    "lint:check": "eslint src --ext .ts",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "pub": "npm version patch && npm run build && npm publish"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.194.0",
    "constructs": "^10.4.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/aws-lambda": "^8.10.149",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.0",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "eslint": "^8.30.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/TheSmartMonkey/aws-cdk-fanout/issues"
  },
  "homepage": "https://github.com/TheSmartMonkey/aws-cdk-fanout#readme",
  "keywords": [
    "aws",
    "aws-cdk",
    "fanout",
    "webhook",
    "cdk-construct"
  ]
}
